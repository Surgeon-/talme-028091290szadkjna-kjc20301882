draw_sprite_ext(spr_window_start_game,0,xx,yy,sprite_scale,sprite_scale,0,c_white,1);

//COMBINATION LIST:
draw_set_alpha(1);
draw_set_valign(fa_top);
draw_set_halign(fa_left);

//LOOP BEGIN:
for (i=scroll; i&lt;min(scroll+9,ds_list_size(dirs)); i+=1) {

 //Small icons:
 //spr=0;
 //draw_sprite_ext(spr,0,xx+9,yy+counter*20+43,0.5,0.5,0,c_white,1); 

 //Clickable text boxes:
 spr=tern(dirs[|i],selected,1,0);
 if gui_draw_sprite_clickable_ext(spr_window_name_box,spr,xx+58,yy+i*40+126,1,2,2,1)=true {
  selected=dirs[|i]
  }

 //Combination names:
 draw_set_color(c_black);
 draw_text(xx+64,yy+130+40*i,dirs[|i]);

end;
//LOOP END;

//Filter title:
draw_set_color(c_black);
draw_text(xx+28,yy+84,"Choose a save file:");

//FILE DETAILS BEGIN:

if (selected&lt;&gt;"") {

 if gui_draw_sprite_clickable_ext(spr_window_button_play,0,xx+498,yy+498,true,2,2,1)=true {
  
  switch (room) begin
  
   case room_menu:
     remove(obj_main_menu);
     ds_list_destroy(dirs); // PEP
     sett_free();
     NSP_free(true);
     
     if (selected = newgame) {
      with (instance_create(0, 0, obj_load_fade)) {
        mode="new";
        name="";
        delay=90;
        md=delay;
        }
      }
      else {
       //game_load_checkpoint(selected);
       with (instance_create(0,0,obj_load_fade)) {
         mode="load";
         name=selected;
         delay=90;
         md=delay;
         }
       }
     instance_destroy(); 
    break;
    
   default: // PEP - Doesn't release 'dirs'
