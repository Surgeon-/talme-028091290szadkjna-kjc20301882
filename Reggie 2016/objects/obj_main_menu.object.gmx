            <string>
var xx, yy, sh = sprite_get_height(spr_mm_button);

var tlerp = (sett_get_value("Interpolation") == "On");

if (!(instance_exists(obj_gui_load_game) or instance_exists(obj_gui_settings))) {

  var a = gui_get_view_position();
  
  xx = a[0] + 20;
  yy = a[3] - (sh*3 + 60);
  
  ftlerp();
  
  if (gui_draw_sprite_clickable(spr_mm_button, 0, xx, yy, true)) {
    instance_create(0,0,obj_gui_load_game);
    }
    
  if (gui_draw_sprite_clickable(spr_mm_button, 1, xx, yy + sh + 20, true)) {
    instance_create(0,0,obj_gui_settings);
    }
    
  if (gui_draw_sprite_clickable(spr_mm_button, 2, xx, yy + 2*sh + 40, true)) {
    game_end();
    }
    
  rtlerp(tlerp);
  
  }
else {
  
  draw_set_colour(c_black);
  draw_set_alpha(0.75);
  draw_rectangle(0,0,gui_width(),gui_height(),false);
  draw_set_alpha(1);

  }
    
